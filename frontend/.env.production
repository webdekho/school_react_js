# Production Environment Configuration
# Copy this file to .env.production in your frontend directory

# API Configuration
REACT_APP_API_BASE_URL=https://yourdomain.com/backend
REACT_APP_ENVIRONMENT=production

# Application Configuration
REACT_APP_APP_NAME=School Management System
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_DEBUG=false
REACT_APP_ENABLE_CONSOLE_LOGS=false

# Security
REACT_APP_ENABLE_DEV_TOOLS=false

# Upload Configuration
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_ALLOWED_FILE_TYPES=jpg,jpeg,png,pdf,doc,docx

# UI Configuration
REACT_APP_ITEMS_PER_PAGE=10
REACT_APP_SESSION_TIMEOUT=7200000

# Third-party Services (if any)
# REACT_APP_GOOGLE_ANALYTICS_ID=
# REACT_APP_SENTRY_DSN=